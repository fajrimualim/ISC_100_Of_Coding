package Day3;

public class Day27 {

    public static void main(String[] args) {

        int bintang = 4;

        for (int i = 1; i <= bintang; i++) {
            for (int j = 0; j < i; j++) {
                System.out.print("*");

            }
            System.out.println("");

        }

    }

}
