package Day3;

import java.util.Scanner;

public class Day30 {

    public static void main(String[] args) {
        Scanner aji = new Scanner(System.in);
        System.out.print("Masukkan nilai n: ");
        int n = aji.nextInt();

        System.out.println();

        for (int i = 1; i <= n; i++) {
            if (i % 3 == 0) {
                System.out.println(i);
            }
        }
    }
}
