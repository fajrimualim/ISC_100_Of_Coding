package Day3;

import java.util.Scanner;

public class Day26 {

    public static void main(String[] args) {
        Scanner aji = new Scanner(System.in);
        System.out.println("masukkan berapa jumlah hewan peliharaanmu :");
        int peserta = aji.nextInt();

        System.out.println("masukkan nama hewan:");
        String hewan = aji.next();

        for (int i = 1; i <= peserta; i++) {
            System.out.println(hewan);
        }

    }
}
