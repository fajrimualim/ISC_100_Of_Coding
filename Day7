public class Day7 {
    public static void main(String[] args) {
        
        int x;
        int y;
        int hasil;
        
        x = 35;
        y = 6;
        
        
        hasil = x += y;
        System.out.println(hasil);
        
        hasil = x -= y;
        System.out.println(hasil);
        
        hasil = x *= y;
        System.out.println(hasil);
        
        hasil = x /= y;
        System.out.println(hasil);
        
        hasil = x %= y;
        System.out.println(hasil);
    }
}
